{
  "name": "crowd-wisdom-sports-react",
  "version": "1.0.0",
  "description": "Stakehouse Sports - A Game that Goes Beyond the Coin Flip: MongoDB, Node.js, React.js",
  "main": "index.js",
  "scripts": {
    "client-install": "yarn install --prefix client",
    "start": "npm run build && node ./dist/server.js --ignore public/",
    "dev": "concurrently --kill-others \"webpack -wd\" \"nodemon --exec babel-node ./src/server.js BABEL_ENV=dev --ignore public/\"",
    "build": "babel ./src --out-dir dist && webpack",
    "server:prod": "node ./dist/server.js",
    "clean": "rimraf dist",
    "prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
    "test": "jest",
    "analyze": "source-map-explorer 'public/bundle.js'",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn install --prefix client && yarn run build --prefix client"
  },
  "repository": "git+https://github.com/cmaronchick/crowd-wisdom-sports-react.git",
  "author": "CMA",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jscomplete/learn-fullstack-javascript/issues"
  },
  "homepage": "https://github.com/jscomplete/learn-fullstack-javascript#readme",
  "dependencies": {
    "@babel/cli": "7.6.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.3",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "aws-amplify": "^1.1.26",
    "aws-amplify-react": "^2.3.6",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jquery": "^3.4.0",
    "mongodb": "^3.1.13",
    "node-sass": "^4.11.0",
    "node-sass-middleware": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-chartjs-2": "^2.7.6",
